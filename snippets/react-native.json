  "RN创建样式": {
    "prefix": "iirnstyle",
    "body": [
      "import { StyleSheet } from 'react-native';",
      "",
      "export default StyleSheet.create({",
      "  container:{ ",
      "    backgroundColor:'white'",
      "  }",
      "});"
    ]
  },
  "RN(Style属性) ==": {
    "prefix": "iistyle",
    "body": ["style={styles.$1}$0"]
  },
  "RN图标": {
    "prefix": "iirnicon",
    "body": ["import { Entypo } from '@expo/vector-icons';"]
  },
