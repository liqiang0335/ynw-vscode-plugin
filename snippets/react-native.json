{
  "RN组件(外部样式)": {
    "prefix": "nnfunc1",
    "body": [
      "import React from 'react';",
      "import { View, Text } from 'react-native';",
      "import styles from './${1:${TM_FILENAME_BASE}}.style'",
      "",
      "export default function ${1:${TM_FILENAME_BASE}}() {",
      "  return (",
      "    <View style={styles.container}>",
      "      <Text>${1:${TM_FILENAME_BASE}}</Text>",
      "    </View>",
      "  );",
      "}",
      ""
    ]
  },
  "RN组件(内部样式)": {
    "prefix": "nnfunc2",
    "body": [
      "import React from 'react';",
      "import { View, Text, StyleSheet } from 'react-native';",
      "",
      "export default function ${1:${TM_FILENAME_BASE}}() {",
      "  return (",
      "    <View style={styles.container}>",
      "      <Text>${1:${TM_FILENAME_BASE}}</Text>",
      "    </View>",
      "  );",
      "}",
      "",
      "const styles = StyleSheet.create({",
      "  container: {",
      "    flex: 1,",
      "    justifyContent: 'center',",
      "    alignItems: 'center',",
      "    backgroundColor: '#2c3e50'",
      "  }",
      "});"
    ]
  },
  "RN函数组件": {
    "prefix": "nnfunc3",
    "body": [
      "import React from 'react';",
      "import { View, Text } from 'react-native';",
      "",
      "export default function FormIntro() {",
      "  return (",
      "    <View >",
      "      <Text>FormIntro</Text>",
      "    </View>",
      "  );",
      "}"
    ]
  },
  "RN创建样式": {
    "prefix": "nnStyleCreate",
    "body": [
      "import { StyleSheet } from 'react-native';",
      "",
      "export default StyleSheet.create({",
      "  container:{ ",
      "    backgroundColor:'white'",
      "  }",
      "});"
    ]
  },
  "RN(Style属性)": {
    "prefix": "nnStyleProperty",
    "body": [
      "style={styles.$1}$0"
    ]
  },
  "RN图标": {
    "prefix": "nnicon",
    "body": [
      "import { Entypo, AntDesign } from '@expo/vector-icons';"
    ]
  },
  "RNText": {
    "prefix": "nnText",
    "body": [
      "<Text style={styles.$2}>$1</Text>$0"
    ]
  },
  "RNImage": {
    "prefix": "nnImage",
    "body": [
      "<Image source={{uri:'$2'}} style={{width:40,height:40}}>$1</Image>"
    ]
  },
  "RNView": {
    "prefix": "nnView",
    "body": [
      "<View style={styles.$2}>$1</View>"
    ]
  },
  "导入ReactNative": {
    "prefix": "nnimrn",
    "body": [
      "import { $0 } from 'react-native';"
    ]
  },
