  "⭕️ RN创建样式": {
    "prefix": "yy.rn.StyleCreate",
    "body": [
      "import { StyleSheet } from 'react-native';",
      "",
      "export default StyleSheet.create({",
      "  container:{ ",
      "    backgroundColor:'white'",
      "  }",
      "});"
    ]
  },
  "⭕️ RN(Style属性)": {
    "prefix": "yy.rn.StyleProperty",
    "body": [
      "style={styles.$1}$0"
    ]
  },
