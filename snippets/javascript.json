{
  "ii.function-property": {
    "prefix": "ii.function.property",
    "body": ["$1() {", "  $2", "},"]
  },
  "ii.mapState": {
    "prefix": "ii.mapstate",
    "body": ["import { mapState } from 'vuex'"]
  },
  "iilog": {
    "prefix": "iilog",
    "body": ["console.log('>> ${1:name}',${1});$0"]
  },
  "iilog2": {
    "prefix": "iilog",
    "body": ["console.log('>> $1';$0"]
  },
  "iilog-color": {
    "prefix": "iilogcolor",
    "body": ["console.log('>> ${1:name}'.bgGreen, ${1});$0"]
  },
  "ii.format-import": {
    "prefix": "ii.format",
    "body": ["import format from 'date-fns/format';"]
  },
  "ii.format": {
    "prefix": "ii.format",
    "body": ["format($1,'yyyy-MM-dd HH:mm');$2"]
  },
  "ii.debounce-import": {
    "prefix": "ii.debounce",
    "body": ["import debounce from 'lodash/debounce';"]
  },
  "ii.debounce-function": {
    "prefix": "ii.debounce",
    "body": [
      "debounce(function() {",
      "\t\t$1",
      "}, 1000, { leading: true, trailing: false })"
    ]
  },
  "==env.dev==": {
    "prefix": "iienvdev",
    "body": ["process.env.NODE_ENV ==\"development\""]
  },
  "==env.pro==": {
    "prefix": "iienvpro",
    "body": ["process.env.NODE_ENV ==\"production\""]
  },
  "==import-module==": {
    "prefix": "iima",
    "body": "import { $2 } from '${1:module}'$0"
  },
  "==import==": {
    "prefix": "iimb",
    "body": "import ${2:moduleName} from '${1:module}'$0"
  },
  "module-import": {
    "prefix": "iimc",
    "body": ["import ${1:name} from '$2/${1}';$0"]
  },
  "==import-api==": {
    "prefix": "iimd",
    "body": ["import * as $1 from '$2';$0"]
  },
  "==import-style==": {
    "prefix": "iims",
    "body": ["import ${1:styles} from './${TM_FILENAME_BASE}.scss';$0"]
  },
  "lodash-import": {
    "prefix": "iilodash",
    "body": ["import ${1:name} from 'lodash/${1}';$0"]
  },
  "date-fns-import": {
    "prefix": "iidate-fns",
    "body": ["import ${1:name} from 'date-fns/${1}';$0"]
  },
  "==function==": {
    "prefix": "iifuncion",
    "body": ["const $1 = ($2) => {", " $0", "};"]
  }
}
