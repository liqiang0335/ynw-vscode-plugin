{
  "ynw.mapState": {
    "prefix": "ii.mapstate",
    "body": ["import { mapState } from 'vuex'"]
  },
  "iilog": {
    "prefix": "iilog",
    "body": ["console.log('>> ${1:name}',${1});$0"]
  },
  "iilog-color": {
    "prefix": "iilogcolor",
    "body": ["console.log('>> ${1:name}'.bgGreen, ${1});$0"]
  },
  "ii.format-import": {
    "prefix": "ii.format",
    "body": ["import format from 'date-fns/format';"]
  },
  "ii.format": {
    "prefix": "ii.format",
    "body": ["format($1,'YYYY-MM-DD HH:mm');$2"]
  },
  "ii.debounce-import": {
    "prefix": "ii.debounce",
    "body": ["import debounce from 'lodash/debounce';"]
  },
  "ii.debounce-function": {
    "prefix": "ii.debounce",
    "body": [
      "debounce(function() {",
      "\t\t$1",
      "}, 1000, { leading: true, trailing: false })"
    ]
  },
  "==env.dev==": {
    "prefix": "iienvdev",
    "body": ["process.env.NODE_ENV ==\"development\""]
  },
  "==env.pro==": {
    "prefix": "iienvpro",
    "body": ["process.env.NODE_ENV ==\"production\""]
  },
  "==import-module==": {
    "prefix": "iima",
    "body": "import { $2 } from '${1:module}'$0"
  },
  "==import==": {
    "prefix": "iimb",
    "body": "import ${2:moduleName} from '${1:module}'$0"
  },
  "module-import": {
    "prefix": "iicc",
    "body": ["import ${1:name} from '$2/${1}';$0"]
  },
  "lodash-import": {
    "prefix": "iilodash",
    "body": ["import ${1:name} from 'lodash/${1}';$0"]
  },
  "date-fns-import": {
    "prefix": "iidate-fns",
    "body": ["import ${1:name} from 'date-fns/${1}';$0"]
  },
  "==function==": {
    "prefix": "iifuncion",
    "body": ["const $1 = ($2) => {", " $0", "};"]
  },
  "express.response": {
    "prefix": "ii.response",
    "body": [".then(data => res.send(response.success(data)))", ".catch(next);"]
  },
  "ant-getFieldDecorator": {
    "prefix": "ant.getFieldDecorator",
    "body": ["getFieldDecorator"]
  },
  "ant-resetFields": {
    "prefix": "ant.resetFields",
    "body": ["resetFields"]
  },
  "ant-setFieldsValue": {
    "prefix": "ant.setFieldsValue",
    "body": ["setFieldsValue"]
  },
  "ant-validateFields": {
    "prefix": "ant.validateFields",
    "body": ["validateFields"]
  }
}
