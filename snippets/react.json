	"⭕️ iird": {
		"prefix": "⭕️ yyrd",
		"body": [
			"import React from 'react';",
			"import { createRoot } from 'react-dom/client';",
			"",
			"const App = () => {",
			"  return <div>Hello</div>;",
			"};",
			"",
			"createRoot(document.getElementById('app')).render(<App />);"
		]
	},
