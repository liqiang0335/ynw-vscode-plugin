{
	"⭕️ 导入styled": {
		"prefix": "iistyled",
		"body": [
			"import styled from 'styled-components';"
		]
	},
	"⭕️ 导入router": {
		"prefix": "iirouter",
		"body": [
			"import { useHistory, NavLink } from 'react-router-dom'"
		]
	},
	"⭕️ 导入useSelector ": {
		"prefix": "iiredux1",
		"body": [
			"import { useSelector } from 'react-redux'"
		]
	},
	"⭕️ 导入useDispatch": {
		"prefix": "iiredux2",
		"body": [
			"import { useDispatch } from 'react-redux'"
		]
	},
	"⭕️ createSelector": {
		"prefix": "iicreateSelector",
		"body": [
			"import { createSelector } from 'reselect';",
			"",
			"export default createSelector(state => state.$1, Handler);",
			"",
			"function Handler($2) {",
			"  $0",
			"}"
		]
	},
	"⭕️ useState": {
		"prefix": "iiuseState",
		"body": [
			"const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState($2)"
		]
	},
	"⭕️ useEffect": {
		"prefix": "iiuseEffect",
		"body": [
			"useEffect(() => {",
			" $1",
			" },",
			"[]",
			")"
		]
	},
	"⭕️ 行内样式": {
		"prefix": "iiclassName",
		"body": "className={styles.$1} $2"
	},
	"⭕️ imr": {
		"prefix": "iir",
		"body": [
			"import React from 'react'"
		]
	},
	"⭕️ imd": {
		"prefix": "iid",
		"body": [
			"import ReactDOM from 'react-dom'"
		]
	},
	"⭕️ 函数组件": {
		"prefix": "iirfc",
		"body": [
			"import React from 'react'",
			"",
			"export default function ${TM_FILENAME_BASE}() {",
			"  return (",
			"    <div>",
			"      ${TM_FILENAME_BASE}",
			"    </div>",
			"  )",
			"}",
			""
		]
	},
	"⭕️ 逻辑组件": {
		"prefix": "iiif",
		"description": "创建逻辑组件、useHook组件",
		"body": [
			"import React, { useReducer, useEffect } from 'react';",
			"import http from '@script/http';",
			"",
			"const initState = {};",
			"const reducer = (state, action) => {",
			"  return { ...state, ...action };",
			"};",
			"",
			"export default function ${TM_FILENAME_BASE}() {",
			"  const [state, dispatch] = useReducer(reducer, initState);",
			"",
			"  useEffect(() => {}, []);",
			"",
			"  return { state, dispatch }",
			"}",
			""
		]
	},
	"⭕️ useContext": {
		"prefix": "iiuseContext",
		"body": [
			"const ${1:context} = useContext(${2:contextValue})"
		]
	},
	"⭕️ useReducer": {
		"prefix": "iiuseReducer",
		"body": [
			"const [state, dispatch] = useReducer(${1:reducer}, ${2:initState})"
		]
	},
	"⭕️ iiuselogic": {
		"prefix": "iiuselogic",
		"body": [
			"const props = useLogic();",
			"const { state, dispatch } = props;"
		]
	},
	"⭕️ useCallback": {
		"prefix": "iiuseCallback",
		"body": [
			"const ${1:fn} = useCallback(",
			"  () => {",
			"    ${0:callback}",
			"  },",
			"  [],",
			")"
		]
	},
	"⭕️ useMemo": {
		"prefix": "iiuseMemo",
		"body": [
			"useMemo(() => ${1:function}, ${2:input})"
		]
	},
	"⭕️ useRef": {
		"prefix": "iiuseRef",
		"body": [
			"const ${1:ref} = useRef(${2:initialValue})"
		]
	},
	"⭕️ 标签div": {
		"prefix": "iidiv",
		"body": [
			"<div className={styles.$1}>$0</div>"
		]
	},
	"⭕️ 标签span": {
		"prefix": "iispan",
		"body": [
			"<span className={styles.$1}>$0</span>"
		]
	},
	"⭕️ imrd": {
		"prefix": "iird",
		"body": [
			"import React from 'react'",
			"import ReactDOM from 'react-dom'",
			"",
			"",
			"const App = () => {",
			"  return (",
			"    <div>",
			"      <div>Hello</div>",
			"    </div>",
			"  )",
			"}",
			"",
			"ReactDOM.render(<App />, document.getElementById('app'))"
		]
	},
	"⭕️ 类型注释PropTypes": {
		"prefix": "iiproptypes",
		"body": [
			"import PropTypes from 'prop-types';",
			"",
			"// func,bool,string,object,element,any,arrayOf,oneOf([]),shape({})",
			"${TM_FILENAME_BASE}.propTypes = {",
			"  number: PropTypes.number.isRequired,",
			"  customProp(props, propName, componentName) {}",
			"};"
		]
	},
	"⭕️ import useSWR": {
		"prefix": "iiswr",
		"body": [
			"import useSWR from 'swr';"
		]
	},
	"⭕️ 创建styled": {
		"prefix": "sss",
		"body": [
			"const S$1 = styled.div`",
			"  $0",
			"`"
		]
	}
}