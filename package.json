    "commands": [{
      "command": "ynw.relavePath",
      "title": "Relative Path for Active"
    }],
    "menus": {
      "explorer/context": [{
        "command": "ynw.relavePath",
        "group": "ynw"
      }]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}