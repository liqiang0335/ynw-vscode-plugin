{
  "name": "ynw",
  "displayName": "ynw",
  "description": "Some Tool for YNW",
  "version": "0.2.7",
  "publisher": "liqiang0335",
  "author": "liqiang0335 <liqiang0335@gmail.com>",
  "bugs": {
    "url": "https://github.com/liqiang0335/ynw-vscode-plugin/issues",
    "email": "liqiang0335@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liqiang0335/ynw-vscode-plugin.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.jpg",
  "keywords": [
    "ynw"
  ],
  "activationEvents": [
    "onCommand:ynw.createJson",
    "onCommand:ynw.fileFactory",
    "onCommand:ynw.createSameNameFile",
    "onCommand:ynw.importModule",
    "onCommand:ynw.relavePath",
    "onCommand:ynw.webpackBuild.dev",
    "onCommand:ynw.webpackBuild.hot",
    "onCommand:ynw.webpackBuild.pro"
  ],
  "main": "./src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "YNW配置文件",
      "properties": {
        "ynw.showJsonMenu": {
          "description": "JSON: 设置显示菜单",
          "type": "boolean",
          "default": false
        },
        "ynw.jsonName": {
          "description": "JSON: 设置文件名称",
          "type": "string",
          "default": "__db__"
        },
        "ynw.jsonIgnore": {
          "description": "JSON: 设置忽略文件夹的正则",
          "type": "string",
          "default": "(src|dist|images?|style|script)"
        },
        "ynw.showFileFactory": {
          "description": "加工文件",
          "type": "boolean",
          "default": false
        },
        "ynw.showImportModuleMenu": {
          "description": "导入模块",
          "type": "boolean",
          "default": true
        },
        "ynw.showCopyRelativePathMenu": {
          "description": "复制相对路径",
          "type": "boolean",
          "default": true
        },
        "ynw.createSameNameFile": {
          "description": "创建同名文件",
          "type": "boolean",
          "default": false
        },
        "ynw.styleFileType": {
          "description": "创建同名样式文件的类型",
          "type": "string",
          "default": "scss",
          "enum": [
            "css",
            "scss",
            "less"
          ]
        }
      },
      "ynw.createStyleFileType": {
        "description": "创建同名样式文件的类型",
        "type": "string",
        "default": "scss",
        "enum": [
          "css",
          "scss",
          "less"
        ]
      }
    },
    "commands": [{
        "command": "ynw.fileFactory",
        "title": "文件加工"
      },
      {
        "command": "ynw.createJson",
        "title": "生成JSON文件"
      },
      {
        "command": "ynw.importModule",
        "title": "导入选中模块"
      },
      {
        "command": "ynw.createSameNameFile",
        "title": "新建同名文件"
      },
      {
        "command": "ynw.relavePath",
        "title": "复制相对路径"
      },
      {
        "command": "ynw.webpackBuild.dev",
        "title": "ynw-cli >> 开发环境"
      },
      {
        "command": "ynw.webpackBuild.hot",
        "title": "ynw-cli >> 热更新模式"
      },
      {
        "command": "ynw.webpackBuild.pro",
        "title": "ynw-cli >> 生产环境"
      }
    ],
    "keybindings": [{
        "command": "ynw.fileFactory",
        "key": "ctrl+y ctrl+c"
      },
      {
        "command": "ynw.createSameNameFile",
        "key": "ctrl+y ctrl+n"
      }
    ],
    "menus": {
      "editor/context": [{
        "command": "ynw.fileFactory"
      }],
      "explorer/context": [{
          "command": "ynw.importModule",
          "when": "config.ynw.showImportModuleMenu",
          "group": "ynw-a@1"
        },
        {
          "command": "ynw.createSameNameFile",
          "when": "config.ynw.createSameNameFile",
          "group": "ynw-a@2"
        },
        {
          "command": "ynw.relavePath",
          "when": "config.ynw.showCopyRelativePathMenu",
          "group": "ynw-a@3"
        },
        {
          "command": "ynw.fileFactory",
          "when": "config.ynw.showFileFactory",
          "group": "ynw-b@1"
        },
        {
          "command": "ynw.createJson",
          "when": "config.ynw.showJsonMenu",
          "group": "ynw-b@2"
        },
        {
          "command": "ynw.webpackBuild.dev",
          "when": "resourceExtname == .js",
          "group": "ynw-c@1"
        },
        {
          "command": "ynw.webpackBuild.hot",
          "when": "resourceExtname == .js",
          "group": "ynw-c@2"
        },
        {
          "command": "ynw.webpackBuild.pro",
          "when": "resourceExtname == .js",
          "group": "ynw-c@3"
        }
      ]
    },
    "snippets": [{
      "language": "vue",
      "path": "./snippets/vue.json"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}