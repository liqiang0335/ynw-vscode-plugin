{
  "name": "ynw",
  "displayName": "ynw",
  "description": "Some Tool for YNW ",
  "version": "0.0.5",
  "publisher": "liqiang0335",
  "author": "liqiang0335 <liqiang0335@gmail.com>",
  "bugs": {
    "url": "https://github.com/liqiang0335/ynw-vscode-plugin/issues",
    "email": "liqiang0335@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liqiang0335/ynw-vscode-plugin.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ynw",
    "relative path",
    "webpack"
  ],
  "activationEvents": [
    "onCommand:ynw.relavePath",
    "onCommand:ynw.webpackBuild.dev",
    "onCommand:ynw.webpackBuild.hot",
    "onCommand:ynw.webpackBuild.pro"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [{
        "command": "ynw.relavePath",
        "title": "Relative Path for Active"
      },
      {
        "command": "ynw.webpackBuild.dev",
        "title": "Ynw Build This - Dev"
      },
      {
        "command": "ynw.webpackBuild.hot",
        "title": "Ynw Build This - Hot"
      },
      {
        "command": "ynw.webpackBuild.pro",
        "title": "Ynw Build This - Pro"
      }
    ],
    "menus": {
      "explorer/context": [{
          "command": "ynw.relavePath",
          "group": "ynw"
        },
        {
          "command": "ynw.webpackBuild.dev",
          "when": "resourceExtname == .js",
          "group": "ynw"
        },
        {
          "command": "ynw.webpackBuild.hot",
          "when": "resourceExtname == .js",
          "group": "ynw"
        },
        {
          "command": "ynw.webpackBuild.pro",
          "when": "resourceExtname == .js",
          "group": "ynw"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}