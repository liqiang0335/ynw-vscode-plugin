{
  "name": "ynw",
  "displayName": "ynw",
  "description": "some tool for ynw ",
  "version": "0.0.2",
  "publisher": "liqiang0335",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ynw.relavePath"
  ],
