{
  "name": "ynw",
  "displayName": "ynw",
  "description": "Some Tool for YNW",
  "version": "0.4.1",
  "publisher": "liqiang0335",
  "author": "liqiang0335 <liqiang0335@gmail.com>",
  "bugs": {
    "url": "https://github.com/liqiang0335/ynw-vscode-plugin/issues",
    "email": "liqiang0335@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liqiang0335/ynw-vscode-plugin.git"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.jpg",
  "keywords": [
    "ynw"
  ],
  "activationEvents": [
    "onCommand:ynw.importModule",
    "onCommand:ynw.relavePath",
    "onCommand:ynw.webpackBuild.dev",
    "onCommand:ynw.webpackBuild.hot",
    "onCommand:ynw.webpackBuild.pro"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "ynw.importModule",
        "title": "导入选中模块"
      },
      {
        "command": "ynw.relavePath",
        "title": "复制相对路径"
      },
      {
        "command": "ynw.webpackBuild.dev",
        "title": "ynw-cli >> 开发环境"
      },
      {
        "command": "ynw.webpackBuild.hot",
        "title": "ynw-cli >> 热更新模式"
      },
      {
        "command": "ynw.webpackBuild.pro",
        "title": "ynw-cli >> 生产环境"
      }
    ],
    "keybindings": [],
    "menus": {
      "editor/context": [],
      "explorer/context": [
        {
          "command": "ynw.importModule",
          "group": "ynw-a@1"
        },
        {
          "command": "ynw.relavePath",
          "group": "ynw-a@2"
        },
        {
          "command": "ynw.webpackBuild.dev",
          "when": "resourceExtname == .js",
          "group": "ynw-c@1"
        },
        {
          "command": "ynw.webpackBuild.hot",
          "when": "resourceExtname == .js",
          "group": "ynw-c@2"
        },
        {
          "command": "ynw.webpackBuild.pro",
          "when": "resourceExtname == .js",
          "group": "ynw-c@3"
        }
      ]
    },
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "copy-paste": "^1.3.0"
  }
}
